SCADE=C:\Program Files\ANSYS Inc\v194\SCADE\SCADE\.

SHELL=cmd.exe

C_FLAGS = -pedantic

all: LandingGear.exe LandingGear.dll 

win64/LandingGear.o: LandingGear.c
	gcc -I"." -I"$(SCADE)" -I"$(SCADE)\include" -I"$(SCADE)\include\C" -I"$(SCADE)\include\Ada" -I"$(SCADE)\lib\Ada" -I"$(SCADE)\libraries\SC65\libmathext" $(C_FLAGS) -DBUILD_DLL -DSIM -DWIN32 -D_CONSOLE -D_MBCS -c -ansi -std=c99 -m64 "LandingGear.c" -o "win64\LandingGear.o"

